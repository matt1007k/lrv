{
  "name": "server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env.development -- nodemon src/index.ts",
    "test": "NODE_ENV=test npm run migrate:test && dotenv -e .env.test jest --forceExit --detectOpenHandles --maxWorkers=1",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name test-init",
    "migrate:init": "dotenv -e .env.development -- npx prisma migrate dev --name init",
    "db:seed": "dotenv -e .env.development -- npx prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^3.3.0",
    "argon2": "^0.28.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "4.17.1",
    "express-validator": "^6.12.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/multer": "^1.4.7",
    "@types/node": "14.17.20",
    "@types/supertest": "^2.0.11",
    "jest": "^27.2.4",
    "nodemon": "^2.0.13",
    "prisma": "^3.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
